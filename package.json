{
	"name": "@zebec-fintech/partners-card-sdk",
	"version": "1.0.0",
	"description": "An sdk for purchasing silver card in zebec",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"author": "Ashish Sapkota",
	"license": "MIT",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "npm run clean && tsc",
		"clean": "rimraf dist",
		"format": "prettier --write .",
